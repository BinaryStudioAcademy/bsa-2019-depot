{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "airbnb-base",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "camelcase": 2,
    "no-unused-vars": 2,
    "eqeqeq": 2,
    "max-len": 0,
    "no-console": [2, { "allow": ["warn", "error"] }],
    "indent": [2, 2],
    "quotes": [2, "single", "avoid-escape"],
    "semi": 2,
    "comma-dangle": "off"
  }
}
